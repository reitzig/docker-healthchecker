[
    {
        "description": "Webservice responds",
        "command": [
            "curl",
            "-f",
            "webservice/contenst.json"
        ],
        "timeout": "1s"
    },
    {
        "description": "Database responds",
        "command": [
            "psql",
            "postgresql://${POSTGRES_USER}:${POSTGRES_PASSWORD}@database/${POSTGRES_DB}",
            "-c", "SELECT COUNT(*) FROM data;"
        ],
        "timeout": "1s"
    }
]
